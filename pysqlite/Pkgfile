# Description: Python bindings for sqlite
# URL: http://www.initd.org/tracker/pysqlite/wiki/pysqlite
# Maintainer: Lucas Hazel, lucas at die dot net dot au
# Depends on: python, sqlite3

name=pysqlite
version=2.3.3
release=1
source=(http://initd.org/pub/software/$name/releases/${version::3}/$version/$name-$version.tar.gz)

build() {
	cd $name-$version
	python setup.py install --root=$PKG
}
